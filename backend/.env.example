# AI Service API Keys
OPENAI_API_KEY=your_openai_api_key_here
STABILITY_API_KEY=your_stability_api_key_here

# Gemini 配置 (中转站 API)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_BASE_URL=https://yunwu.ai
# Gemini 模型选项 (可在前端选择):
# - gemini-2.5-flash-image (推荐，快速生成)
# - gemini-3-pro-image-preview (高级版本，支持 4K、Google Search)
GEMINI_MODEL=gemini-2.5-flash-image
GEMINI_AUTH_MODE=auto  # auto, x-goog, bearer

# Doubao 配置
DOUBAO_API_KEY=your_doubao_api_key_here
DOUBAO_BASE_URL=https://yunwu.ai
DOUBAO_MODEL=doubao-seedream-4-0-250828

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=True

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,http://127.0.0.1:5500,http://localhost:5500
